# Application
NODE_ENV=development
PORT=3000
AUTH_PORT=3001

# Database
DATABASE_URL=postgres://user:password@localhost:5432/dragic

# JWT
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=1h

# Auth Guards
ENABLE_JWT_GUARD=true
ENABLE_RESOURCE_GUARD=true

# Cache
CACHE_SERVICE=redis
# Redis
REDIS_URL=redis://localhost:6379
# Vercel KV (alternative)
# CACHE_SERVICE=vercel_kv
# KV_URL=
# KV_REST_API_TOKEN=

# Storage
STORAGE_SERVICE=minio
# MinIO
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=dragic
MINIO_PUBLIC_BASE_URL=http://localhost:9000/dragic
# Vercel Blob (alternative)
# STORAGE_SERVICE=vercel_blob
# VERCEL_BLOB_TOKEN=
# VERCEL_BLOB_BASE_URL=

# OAuth - Feishu
FEISHU_APP_ID=
FEISHU_APP_SECRET=
FEISHU_REDIRECT_URI=http://localhost:3001/auth/feishu/callback

# OIDC Clients
# NocoDB
NOCODB_CLIENT_ID=nocodb
NOCODB_CLIENT_SECRET=your-nocodb-client-secret
NOCODB_REDIRECT_URI=http://localhost:8080/api/v1/auth/callback
