{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@pictode-api/storage/(.*)": "<rootDir>/../libs/storage/src/$1",
    "@pictode-api/storage": "<rootDir>/../libs/storage/src"
  }
}