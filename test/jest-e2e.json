{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@pictode-api/storage/(.*)": "<rootDir>/../libs/storage/src/$1",
    "@pictode-api/storage": "<rootDir>/../libs/storage/src",
    "@pictode-api/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@pictode-api/auth": "<rootDir>/../libs/auth/src",
    "@pictode-api/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@pictode-api/prisma": "<rootDir>/../libs/prisma/src",
    "@pictode-image-api/cache/(.*)": "<rootDir>/../libs/cache/src/$1",
    "@pictode-image-api/cache": "<rootDir>/../libs/cache/src"
  }
}